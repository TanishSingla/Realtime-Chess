{
  "version": 2,
  "builds": [
    {
      "src": "api/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "views/index.ejs",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/socket",
      "dest": "/api/app.js"
    },
    {
      "src": "/",
      "dest": "/views/index.ejs"
    }
  ]
}
